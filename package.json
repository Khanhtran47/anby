{
  "name": "zzz",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "pnpm build:icons",
    "dev": "next dev --turbopack",
    "build": "run-s \"build:*\"",
    "build:icons": "icons build -i resources/svg-icons -o components/ui/icons --optimize",
    "build:app": "next build",
    "start": "next start",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "pnpm run lint --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "format-code": "pnpm run prettier:fix && pnpm run lint:fix",
    "typecheck": "tsc --noEmit",
    "unused": "knip --max-issues 1",
    "unused:fix": "knip --fix"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.5",
    "@fontsource/commit-mono": "^5.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.509.0",
    "next": "15.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@khanhtran47/config": "1.0.7",
    "@next/eslint-plugin-next": "^15.3.2",
    "@tailwindcss/postcss": "4.1.6",
    "@types/node": "22.15.17",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "babel-plugin-react-compiler": "19.1.0-rc.1",
    "eslint": "9.26.0",
    "eslint-config-next": "15.3.2",
    "knip": "5.55.1",
    "npm-run-all": "^4.1.5",
    "prettier": "3.5.3",
    "prettier-plugin-sort-json": "4.1.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "svg-icons-cli": "^0.0.8",
    "svgo": "^3.3.2",
    "tailwindcss": "4.1.6",
    "tw-animate-css": "^1.2.9",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.10.0"
}
