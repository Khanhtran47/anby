{
  "name": "zzz",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "pnpm build:icons",
    "dev": "next dev --turbopack",
    "build": "run-s \"build:*\"",
    "build:icons": "icons build -i resources/svg-icons -o components/ui/icons --optimize",
    "build:app": "next build",
    "start": "next start",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "pnpm run lint --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "format-code": "pnpm run prettier:fix && pnpm run lint:fix",
    "typecheck": "tsc --noEmit",
    "unused": "knip --max-issues 1",
    "unused:fix": "knip --fix",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "pnpm build:icons && storybook build"
  },
  "dependencies": {
